header.WelcomeHero{
    background-color: #2a6b82;
    color: white;
    /*font-family: 宋体;*/
    .Hero-subtitle{
        text-align: left;
    }
}
.Afrux-NewsWidget &gt; .AfruxWidgets-Widget-content{
    transition: background-color 300ms ease;
}
.Afrux-NewsWidget &gt; .AfruxWidgets-Widget-content:hover{
    background-color: green;
    color: white;
    a{
        color: yellow;
    }
}
.Afrux-TopPostersWidget-users-item-name{
    font-size: normal;
}

div.DiffList-container .DiffList-content&gt;ul&gt;li.Diff:hover{
    background: #ccc;
}

div.DiscussionListItem{
    border-radius: 10px;
}

/*header.WelcomeHero:hover{
    transform: skewx(2deg) translatey(5px);
    transform-origin: bottom right;
    perspective: 500px;
}
*/

button.IndexPage-newDiscussion.Button--primary{
    background: gold;
    color: black;
    transition: background,box-shadow 200ms ease-in-out;
    &amp;:hover{
        background: green;
        color: white;
        box-shadow: 3px 3px 5px gold;
    }
}

li.item-hide button.hasIcon{
    &amp;:hover{
        background-color: red;
        color: white;
    }
}
li.item-sticky button.hasIcon{
    &amp;:hover{
        background-color: #6b27a9;
        color: white;
    }
}

li.item-clarkwinkelmann-author-change button.hasIcon{
    &amp;:hover{
        background-color: #a2dcd2;
    }
}

li.item-shadow-hide button.hasIcon{
    &amp;:hover{
        color: #b2d0bf;
        font-weight: bold;
    }
}

li.item-like button.Button.Button--link{
    font-size: large;
    &amp;:hover{
        color: green;
        font-weight: bold;
    }
}

li.item-logOut button.hasIcon{
    font-size: small;
    &amp;:hover{
        background-color: white;
        font-weight: 100;
        color: red;
    }
    &amp;:after{
        content: "（别离开啊~）";
        color: black;
    }
}

